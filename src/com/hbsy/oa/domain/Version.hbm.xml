<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hbsy.oa.domain.Version" table="version">
		<id name="vid" type="java.lang.Long" length="5">
			<generator class="increment"></generator>
		</id>
		
		<property name="updatetime" type="java.util.Date"></property>
		<property name="versionid" type="java.lang.Long" length="5"></property>
		
		<property name="title" type="java.lang.String" length="20"></property>
		
		<property name="content" type="java.lang.String" length="100"></property>
		
		<property name="state" type="java.lang.Boolean"></property>
		
		<many-to-one name="kynamic" class="com.hbsy.oa.domain.Kynamic" column="kid"></many-to-one>
	</class>
</hibernate-mapping>