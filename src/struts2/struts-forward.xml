<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<package name="forward" namespace="/" extends="struts-default">
		<action name="forwardAction_forward" method="forward" class="com.hbsy.oa.action.ForwardAction">
			<result name="forward">WEB-INF/jsp/frame/%{#forward}.jsp</result>
		</action>
		<action name="kynamicAction" method="kynamic" class="com.hbsy.oa.action.ForwardAction">
			<result name="kynamic">WEB-INF/jsp/kynamic/kynamic.jsp</result>
		</action>
	</package>
</struts>	